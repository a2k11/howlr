<h3>Howlr Dashboard User Page</h3>

<cite>
  <b>
    Howl at the Moon?
  </b>
</cite>
<fieldset>
  <%= form_for @wordhowl do |f| %>
    <%= f.text_field :body, placeholder: "Enter Howl" %>
    <%= f.submit %>
  <% end %>
  <%= form_for @imagehowl, url: image_howls_path, html: {multipart: true} do |f| %>
      <%= f.file_field :avatar %>
      <%= f.submit %>
    <% end %>
</fieldset>
<br>
<br>
<div>
  <%= render current_user.followed_users %>
</div>

